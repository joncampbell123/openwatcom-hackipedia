proj_name       = ncurses
inc_dirs        = -I"../h"

!include cproj.mif
!include deftarg.mif
!include defrule.mif

.c: ../c

ncurses_objs = &
    access.obj &
    comp_err.obj &
    doalloc.obj &
    fallback.obj &
    free_tty.obj &
    getenv_n.obj &
    home_ter.obj &
    lib_baud.obj &
    lib_beep.obj &
    lib_cur_.obj &
    lib_napm.obj &
    lib_setu.obj &
    lib_tpar.obj &
    lib_tput.obj &
    lib_ttyf.obj &
    read_ent.obj


ncurses.lib : $(ncurses_objs)
!ifdef bootstrap
    ar crs $@ $<
!else
    rm -f $^&.lbc
    @for %i in ($<) do @%append $^&.lbc +%i
    $(librarian) $(libflags) @$^&.lbc
!endif
