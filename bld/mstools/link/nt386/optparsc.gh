				    }
				    return( 1 );
				}
				return( 1 );
			    }
			    return( 1 );
			}
			if( parse_debug( &(data->debug_value) ) ) {
			    data->debug = 1;
			    handle_debug( data, 1 );
			}
			return( 0 );
		    }
		    return( 1 );
		}
		return( 1 );
	    }
	    if( OPT_RECOG_LOWER( 'f' ) ) {
		if( OPT_RECOG_LOWER( 'a' ) ) {
		    if( OPT_RECOG_LOWER( 'u' ) ) {
			if( OPT_RECOG_LOWER( 'l' ) ) {
			    if( OPT_RECOG_LOWER( 't' ) ) {
				if( OPT_RECOG_LOWER( 'l' ) ) {
				    if( OPT_RECOG_LOWER( 'i' ) ) {
					if( OPT_RECOG_LOWER( 'b' ) ) {
					    if( parse_defaultlib( &(data->defaultlib_value) ) ) {
						data->defaultlib = 1;
					    }
					    return( 0 );
					}
					return( 1 );
				    }
				    return( 1 );
				}
				return( 1 );
			    }
			    return( 1 );
			}
			return( 1 );
		    }
		    return( 1 );
		}
		if( parse_def( &(data->def_value) ) ) {
		    data->def = 1;
		}
		return( 0 );
	    }
	    return( 1 );
	}
	return( 1 );
    case 'c':
	if( OPT_RECOG_LOWER( 'o' ) ) {
	    if( OPT_RECOG_LOWER( 'm' ) ) {
		if( OPT_RECOG_LOWER( 'm' ) ) {
		    if( OPT_RECOG_LOWER( 'e' ) ) {
