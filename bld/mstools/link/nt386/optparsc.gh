	return( 1 );
    case 'a':
	if( OPT_RECOG_LOWER( 'l' ) ) {
	    if( OPT_RECOG_LOWER( 'i' ) ) {
		if( OPT_RECOG_LOWER( 'g' ) ) {
		    if( OPT_RECOG_LOWER( 'n' ) ) {
			if( OPT_GET_NUMBER( &(data->align_value) ) ) {
			    check_align( &(data->align_value) );
