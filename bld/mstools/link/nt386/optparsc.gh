	return( 1 );
    case '1':
	if( OPT_RECOG_LOWER( '0' ) ) {
	    if( OPT_RECOG_LOWER( 'x' ) ) {
		if( OPT_RECOG_LOWER( '_' ) ) {
		    data->_10x = 1;
		    return( 0 );
		}
		return( 1 );
	    }
	    return( 1 );
	}
	return( 1 );
    }
    OPT_UNGET();
    return( 1 );
}
