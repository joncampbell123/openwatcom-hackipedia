	if( OPT_RECOG_LOWER( 'e' ) ) {
	    if( OPT_RECOG_LOWER( 'b' ) ) {
		if( OPT_RECOG_LOWER( 'u' ) ) {
		    if( OPT_RECOG_LOWER( 'g' ) ) {
